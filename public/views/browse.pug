extends ./includes/layout.pug

block content
  .jumbotron.jumbotron-fluid
    .container
      h1.display-3 Browse
      p.lead
        | Here is all the available passes.
  .row
    .col-12
      ul.list-group
        each pass in passes.rows
          li.list-group-item
            a(href='/_browse/'+pass.doc._id)=pass.doc.description
